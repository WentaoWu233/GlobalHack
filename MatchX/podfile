# platform :ios, '9.0'

target 'MatchX' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MatchX
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod 'GooglePlacePicker'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  target 'MatchXTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MatchXUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
